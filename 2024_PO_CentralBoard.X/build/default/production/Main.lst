.................... void ws2812_wheel(uint8_t pos, uint8_t* re, uint8_t* gr, uint8_t* bl) 
.................... {
....................     if (pos < 85) 
....................     {
....................         *re = pos * 3;
....................         *gr = 255 - pos * 3;
....................         *bl = 0;
....................     } 
....................     else if (pos < 170) 
....................     {
....................         pos -= 85;
....................         *re = 255 - pos * 3;
....................         *gr = 0;
....................         *bl = pos * 3;
....................     } 
....................     else 
....................     {
....................         pos -= 170;
....................         *re = 0;
....................         *gr = pos * 3;
....................         *bl = 255 - pos * 3;
....................     }
.................... }
